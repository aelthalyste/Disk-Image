<Window x:Class="LicanseKeyAndDemo.LicenseControllerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LicanseKeyAndDemo"
        mc:Ignorable="d"
        Title="LicenseControllerWindow" Height="180" Width="800">
    <Grid>
        <StackPanel Margin="10">
            <RadioButton Name="rbDemo" Content="Demo" GroupName="licenseGroup" IsChecked="True"/>
            <StackPanel Margin="0,10">
                <RadioButton Name="rbLicense" Content="Key" GroupName="licenseGroup"/>
                <TextBox Margin="10"/>
            </StackPanel>
            <Button Name="save" Content="Kaydet" Width="100" Click="save_Click"/>
        </StackPanel>
    </Grid>
</Window>
